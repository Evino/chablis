{
  "name": "chablis",
  "version": "0.1.0",
  "description": "A mobile first frontend framework made with wine",
  "main": "dist/chablis.js",
  "scripts": {
    "storybook": "build-storybook -c .storybook -o docs",
    "start": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "lint": "eslint src/**; exit 0",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "publish": "./scripts/publish.sh",
    "setup": "chmod +x scripts/publish.sh"
  },
  "keywords": [
    "react",
    "chablis",
    "wine",
    "evino",
    "frontend"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Evino/chablis.git"
  },
  "author": "The Evino Frontend Team",
  "contributors": [
    "Evino"
  ],
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-info": "^3.3.12",
    "@storybook/react": "^3.3.11",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.1.4",
    "pre-commit": "^1.2.2",
    "rollup": "^0.55.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.1.5"
  },
  "pre-commit": {
    "run": "publish"
  }
}
