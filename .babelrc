{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "transform-class-properties"
  ],
  "env": {
    "production": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ],
        "stage-0",
        "react"
      ],
      "plugins": [
        "babel-plugin-styled-components",
        "transform-class-properties"
      ]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ],
        "stage-0",
        "react"
      ],
      "plugins": [
        "babel-plugin-styled-components",
        "transform-class-properties"
      ]
    }
  }
}
